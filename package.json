{
  "name": "joogie-mc-server",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "cache:clean": "",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx,css,html}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
